<%- include ../partials/header.ejs %>

<%- include ../partials/main_nav.ejs %>

<!-- Content for the Add page -->
<div class="container">
    <div class="row">
        <div class="offset-md-3 col-md-6">
            <h1><%= title %></h1>

            <form class="form" method="POST">
                <div class="form-group">
                    <label for="CnameTextField">Contact Name</label>
                    <input type="text" class="form-control"
                    id="CnameTextField"
                    placeholder="Enter the Contact Name"
                    name="Cname" required>
                </div>

                <div class="form-group">
                    <label for="CNumberTextField">Contact Number</label>
                    <input type="text" class="form-control"
                    id="CNumberTextField"
                    placeholder="Enter the Contact Number"
                    name="Cnumber" required>
                </div>

                <div class="form-group">
                    <label for="EmailTextField">Email ID</label>
                    <input type="text" class="form-control"
                    id="EmailTextField"
                    placeholder="Enter the Email Address"
                    name="email" required>
                </div>

                <button class="btn btn-primary" type="submit">
                    <i class="fas fa-edit"></i> Submit</button>
                
                    <a href="/business-list" class="btn btn-warning">
                        <i class="fas fa-undo"></i> Cancel</a>

            </form>
        </div>

    </div>
</div>



<%- include ../partials/bottom_nav.ejs %>  

<%- include ../partials/footer.ejs %>